#!/bin/sh

if [ $# -ne 0 ]
then
    DOCDIRS="$*"
else
    DOCDIRS=`cat docdirs`
fi

echo > all_sub_files
for dir in $DOCDIRS
do
    ( cd $dir; ls -1 >> ../all_sub_files)
done

sort all_sub_files | uniq -d |
sed -e '/cgal\.bib/d' -e '/htmlfiles/d' -e '/main\.aux/d' -e '/main\.dvi/d' -e '/main\.log/d' -e '/main\.ps/d' -e '/main\.tex/d' -e '/html_wrapper/d'

rm all_sub_files


